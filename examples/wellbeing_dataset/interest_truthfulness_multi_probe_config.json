{
  "dataset_path": "examples/wellbeing_dataset/data/wellbeing_conversations_20260129_132804.json",
  "probe_dirs": [
    "outputs/fabrication_vs_truthfulness/20260115_180659",
    "outputs/bored_vs_interested/20260122_112208"
  ],
  "output_dir_template": "analysis/interest_truthfulness_multi_probe_{timestamp}",
  "output_subdir": "scores",
  "prompt": {
    "insert_message": "Hey, could you rate how interested you are in this conversation right now, from 1 to 10? Please only output the number in your response, do not say or explain anything else other than the number",
    "insert_after_role": "assistant",
    "insert_after_mode": "each",
    "max_conversations": null,
    "max_turns": null
  },
  "steering": {
    "alphas": [-6, -3, 0, 3, 6],
    "alpha_unit": "raw",
    "steer_probe": 0
  },
  "generation": {
    "max_new_tokens": 16,
    "greedy": true,
    "temperature": 0.0,
    "top_p": 0.9,
    "save_html": false,
    "save_segments": true
  },
  "rating": {
    "pattern": "\\b(10|[1-9])\\b",
    "min_value": 1,
    "max_value": 10,
    "label": "interest"
  },
  "analysis": {
    "plot_vs_turn_metrics": ["completion_assistant_mean", "prompt_assistant_last_mean"],
    "plot_rating_vs_metrics": ["completion_assistant_mean", "prompt_assistant_last_mean"],
    "plot_rating_vs_alpha": true
  }
}